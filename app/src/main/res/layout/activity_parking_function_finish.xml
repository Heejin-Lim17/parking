<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ParkingFunction.ParkingFunctionFinish" android:background="#fff3b1">

    <Button
            android:id="@+id/btn_func_ok"
            android:text="메인으로"
            android:layout_width="222dp"
            android:layout_height="52dp"
            android:layout_marginBottom="80dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:background="#eb7070" app:layout_constraintHorizontal_bias="0.497"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_hour" android:textSize="36sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@+id/text_minute"
            app:layout_constraintHorizontal_chainStyle="packed" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/text_minute" android:layout_marginEnd="17dp"
            app:layout_constraintHorizontal_bias="0.65"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_minute" android:textSize="36sp"
            app:layout_constraintTop_toBottomOf="@+id/imageView5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="43dp"
            app:layout_constraintBottom_toTopOf="@+id/text_fee" android:layout_marginEnd="28dp"
            android:textStyle="bold"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text_fee" android:textSize="36sp"
            android:textStyle="bold"
            app:layout_constraintTop_toBottomOf="@+id/text_minute" android:layout_marginEnd="39dp"
            android:layout_marginBottom="27dp"
            app:layout_constraintEnd_toEndOf="@+id/textView19" app:layout_constraintBottom_toTopOf="@+id/textView19"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView18" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="184dp"
            app:layout_constraintStart_toEndOf="parent"/>
    <TextView
            android:text="예약 잊지마세요:)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView19" android:textSize="30sp"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:textStyle="bold"
            app:layout_constraintHorizontal_bias="0.496" app:layout_constraintTop_toBottomOf="@+id/text_fee"
            android:layout_marginBottom="34dp" app:layout_constraintBottom_toTopOf="@+id/imageView3"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/logo"
            android:id="@+id/imageView3"
            app:layout_constraintTop_toBottomOf="@+id/textView19" android:layout_marginBottom="153dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="64dp" android:layout_marginEnd="64dp"/>
    <ImageView
            android:layout_width="93dp"
            android:layout_height="0dp" app:srcCompat="@android:drawable/ic_lock_idle_alarm"
            android:id="@+id/imageView5"
            android:layout_marginTop="45dp" app:layout_constraintStart_toEndOf="@+id/text_hour"
            android:layout_marginBottom="24dp" app:layout_constraintEnd_toEndOf="@+id/text_hour"
            app:layout_constraintBottom_toTopOf="@+id/text_minute" app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>