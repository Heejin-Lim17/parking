<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Group.SearchGroup.GroupSignupActivity" android:background="#fff3b1">
    <TextView
            android:text="그룹 비밀번호를 입력해주세요!"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="165dp"
            tools:layout_editor_absoluteX="52dp" android:id="@+id/textView16" android:textSize="24sp"
            android:textColor="#7c0a02"/>
    <EditText
            android:layout_width="284dp"
            android:layout_height="52dp"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="257dp" tools:layout_editor_absoluteX="63dp" android:id="@+id/gsignup_name"
            android:textAlignment="center" android:hint="그룹 담당자에게 받은 암호를 입력"/>
    <Button
            android:text="가입하기"
            android:layout_width="210dp"
            android:layout_height="53dp" tools:layout_editor_absoluteY="467dp"
            tools:layout_editor_absoluteX="100dp" android:id="@+id/gsignup_ok" android:background="#eb7070"
            android:textSize="18sp" android:textStyle="bold"/>
    <Button
            android:text="취소"
            android:layout_width="210dp"
            android:layout_height="53dp" tools:layout_editor_absoluteY="545dp"
            tools:layout_editor_absoluteX="99dp" android:id="@+id/gsignup_cancel" android:background="#eb7070"
            android:textSize="18sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>