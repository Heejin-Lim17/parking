<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ParkingFunction.ParkingFunction" android:background="#fff3b1">

    <Button
            android:id="@+id/btn_func_cancel"
            android:text="취소"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBaseline_toBaselineOf="@+id/btn_func_oks" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="92dp"/>
    <Button
            android:id="@+id/btn_func_oks"
            android:text="확인"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="112dp"
            android:layout_marginBottom="164dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="주차 조건 설정"
            android:textColor="#444444"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:textAppearance="@android:style/TextAppearance.Large"
            android:id="@+id/textView9" android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.542" app:layout_constraintVertical_bias="0.185"
            android:textStyle="bold"/>
    <LinearLayout
            android:layout_width="354dp"
            android:layout_height="57dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="92dp"
            app:layout_constraintTop_toBottomOf="@+id/textView9" app:layout_constraintHorizontal_bias="0.487"
            android:id="@+id/linearLayout4">
        <TextView
                android:text="시간"
                android:layout_width="151dp"
                android:layout_height="45dp" android:id="@+id/textView11" android:layout_weight="1"
                android:textAppearance="@android:style/TextAppearance.Large" android:textAlignment="center"/>
        <Spinner
                android:layout_width="192dp"
                android:layout_height="45dp" android:id="@+id/spin_hour" android:layout_weight="1"/>
        <Spinner
                android:layout_width="196dp"
                android:layout_height="45dp" android:id="@+id/spin_minute" android:layout_weight="1"/>
    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="353dp"
            android:layout_height="65dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout4">
        <TextView
                android:text="요금"
                android:layout_width="49dp"
                android:layout_height="45dp" android:id="@+id/textView12" android:layout_weight="1"
                android:textAppearance="@android:style/TextAppearance.Large" android:textAlignment="center"/>
        <Spinner
                android:layout_width="219dp"
                android:layout_height="45dp" android:id="@+id/spin_fee" android:layout_weight="1"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>