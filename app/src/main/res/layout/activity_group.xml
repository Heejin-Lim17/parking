<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Group.MainGroup.GroupActivity" android:background="#fff3b1">


    <LinearLayout
            android:orientation="vertical"
            android:layout_width="341dp"
            android:layout_height="0dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginBottom="116dp"
            app:layout_constraintBottom_toBottomOf="parent" android:id="@+id/linearLayout9"
            >
        <ListView
                android:layout_width="341dp"
                android:listSelector="#f1eb9a"
                android:layout_height="286dp" android:id="@+id/group_listview" android:background="#f4f6f6"
                android:divider="#393e46" android:dividerHeight="1dp"/>
    </LinearLayout>
    <ImageView
            android:layout_width="189dp"
            android:layout_height="179dp" app:srcCompat="@drawable/group_logo"
            android:id="@+id/imageView2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="76dp"
            app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>