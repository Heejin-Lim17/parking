<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Customer.MyInfoActivity" android:background="#fff3b1">

    <TextView
            android:text="회원정보"
            android:textColor="#444444"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:textAppearance="@android:style/TextAppearance.Large"
            android:id="@+id/textView" android:layout_marginTop="80dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="이름"
            android:textColor="#444444"
            android:gravity="right"
            android:layout_width="90dp"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            app:layout_constraintBaseline_toBaselineOf="@+id/myinfo_name" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/myinfo_name"/>
    <EditText
            android:id="@+id/myinfo_name"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="48dp" android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toTopOf="@+id/car_num2"/>
    <TextView
            android:text="차량번호"
            android:textColor="#444444"
            android:gravity="right"
            android:layout_width="90dp"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:layout_marginStart="56dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/car_num2"/>
    <EditText
            android:id="@+id/car_num2"
            android:inputType="text"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toEndOf="@+id/textView4" android:layout_marginStart="12dp"
            app:layout_constraintBottom_toTopOf="@+id/signup_email2"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.111" android:layout_marginBottom="12dp"/>
    <TextView
            android:text="이메일"
            android:textColor="#444444"
            android:gravity="right"
            android:layout_width="90dp"
            android:layout_height="wrap_content" android:id="@+id/textView5"
            android:layout_marginStart="36dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/signup_email2"/>
    <EditText
            android:id="@+id/signup_email2"
            android:ems="20"
            android:inputType="textEmailAddress"
            android:layout_width="202dp"
            android:layout_height="49dp"
            android:layout_marginBottom="56dp"
            app:layout_constraintBottom_toTopOf="@+id/btn_myinfo_finish"
            app:layout_constraintStart_toEndOf="@+id/textView5" android:layout_marginStart="36dp"/>
    <TextView
            android:text="암호"
            android:textColor="#444444"
            android:gravity="right"
            android:layout_width="90dp"
            android:layout_height="wrap_content" android:id="@+id/textView6"
            app:layout_constraintBaseline_toBaselineOf="@+id/myinfo_pw" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/myinfo_pw"/>
    <EditText
            android:id="@+id/myinfo_pw"
            android:ems="8"
            android:inputType="textPassword"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="48dp" android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/btn_myinfo_sel"/>
    <TextView
            android:text="사진"
            android:textColor="#444444"
            android:gravity="right"
            android:layout_width="90dp"
            android:layout_height="wrap_content" android:id="@+id/textView7"
            app:layout_constraintBaseline_toBaselineOf="@+id/btn_myinfo_sel" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/btn_myinfo_sel"/>
    <Button
            android:id="@+id/btn_myinfo_sel"
            android:text="Select"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="160dp"
            android:layout_marginBottom="24dp"
            app:layout_constraintBottom_toTopOf="@+id/myinfo_name"/>
    <Button
            android:id="@+id/btn_myinfo_ok"
            android:text="수정완료"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="116dp"
            android:layout_marginBottom="88dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintBaseline_toBaselineOf="@+id/btn_myinfo_finish"/>
    <Button
            android:id="@+id/btn_myinfo_finish"
            android:text="취소"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="92dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toEndOf="@+id/btn_myinfo_ok"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="아이디"
            android:textColor="#444444"
            android:gravity="right"
            android:layout_width="98dp"
            android:layout_height="wrap_content" android:id="@+id/textView8"
            app:layout_constraintBaseline_toBaselineOf="@+id/myinfo_id" android:layout_marginStart="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/myinfo_id"/>
    <EditText
            android:id="@+id/myinfo_id"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="52dp" android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toTopOf="@+id/myinfo_pw"/>
</androidx.constraintlayout.widget.ConstraintLayout>