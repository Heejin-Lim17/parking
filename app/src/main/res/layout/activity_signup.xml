<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Customer.SignupActivity" android:background="#fff3b1">

    <LinearLayout
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="100dp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginBottom="28dp"
            android:id="@+id/linearLayout8">

        <TextView
                android:text="회원가입"
                android:textColor="#444444"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:textAppearance="@android:style/TextAppearance.Large"/>

    </LinearLayout>

    <LinearLayout
            android:padding="8dp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout3"
            android:layout_marginTop="136dp" android:layout_marginBottom="21dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toTopOf="@+id/linearLayout7"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0">
        <TextView
                android:text="이름"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_name"
                android:hint="Name"
                android:layout_width="200dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="8dp"
            android:layout_height="wrap_content"
            android:id="@+id/tttttttttttttttttttttttttttttttt"
            android:layout_width="401dp"
            tools:layout_editor_absoluteY="516dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="1.0">
        <TextView
                android:text="차량번호"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_car_no"
                android:hint="00가0000"
                android:inputType="text"
                android:layout_width="217dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="8dp"
            android:layout_width="402dp"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout5"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/tttttttttttttttttttttttttttttttt"
            app:layout_constraintHorizontal_bias="0.714">
        <TextView
                android:text="전화번호"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_phone_no"
                android:hint="11자리 번호입력"
                android:ems="20"
                android:inputType="none"
                android:layout_width="214dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout10"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/linearLayout5">
        <TextView
                android:text="암호"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_pw"
                android:hint="Password"
                android:ems="8"
                android:inputType="textPassword"
                android:layout_width="211dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="16dp"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:id="@+id/linearLayout6"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tttttttttttttttttttttttttttttttt"
            android:layout_marginBottom="40dp">
        <Button
                android:id="@+id/btn_signup_ok"
                android:text="확인"
                android:layout_width="114dp"
                android:layout_height="wrap_content" android:background="#eb7070"/>
        <Button
                android:id="@+id/btn_signup_cancel"
                android:text="취소"
                android:layout_width="116dp"
                android:layout_height="wrap_content" android:background="#f8a978"/>
    </LinearLayout>
    <LinearLayout
            android:layout_width="405dp"
            android:layout_height="59dp"
            android:id="@+id/linearLayout7"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="36dp" app:layout_constraintBottom_toTopOf="@+id/linearLayout10"
    >
        <TextView
                android:text="아이디"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="98dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_id"
                android:hint="ID"
                android:layout_width="200dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>