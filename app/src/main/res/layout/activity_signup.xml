<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Customer.SignupActivity" android:background="#fff3b1">

    <LinearLayout
            android:layout_width="match_parent"
            android:gravity="center"
            android:layout_height="100dp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginBottom="8dp"
            android:id="@+id/linearLayout8">

        <TextView
                android:text="회원가입"
                android:textColor="#444444"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:textAppearance="@android:style/TextAppearance.Large"/>

    </LinearLayout>

    <LinearLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="128dp"
            app:layout_constraintTop_toTopOf="parent">
        <TextView
                android:text="이름"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_name"
                android:hint="Name"
                android:layout_width="200dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayout5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/linearLayoutb"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="1.0"
            android:id="@+id/linearLayoutb">
        <TextView
                android:text="차량번호"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/car_num"
                android:hint="00가0000"
                android:inputType="text"
                android:layout_width="200dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout5" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="159dp"
            app:layout_constraintBottom_toBottomOf="parent">
        <TextView
                android:text="이메일"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_email"
                android:hint="Email"
                android:ems="20"
                android:inputType="textEmailAddress"
                android:layout_width="250dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayoutb" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginBottom="88dp" app:layout_constraintBottom_toTopOf="@+id/linearLayout5">
        <TextView
                android:text="암호"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_pw"
                android:hint="Password"
                android:ems="8"
                android:inputType="textPassword"
                android:layout_width="200dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="8dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/linearLayoutb" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="1.0"
            android:id="@+id/linearLayout6" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout7" app:layout_constraintVertical_bias="0.034">
        <TextView
                android:text="사진"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="90dp"
                android:layout_height="wrap_content"/>
        <Button
                android:id="@+id/btn_photo"
                android:text="Select"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:padding="16dp"
            android:gravity="center_horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0" android:id="@+id/linearLayoutb">
        <Button
                android:id="@+id/btn_signup_ok"
                android:text="확인"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        <Button
                android:id="@+id/btn_signup_cancel"
                android:text="취소"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
    </LinearLayout>
    <LinearLayout
            android:layout_width="0dp"
            android:layout_height="58dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout3" android:id="@+id/linearLayout7"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp"
    >
        <TextView
                android:text="아이디"
                android:textColor="#444444"
                android:gravity="right"
                android:layout_width="98dp"
                android:layout_height="wrap_content"/>
        <EditText
                android:id="@+id/signup_id"
                android:hint="ID"
                android:layout_width="200dp"
                android:layout_height="wrap_content"/>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>